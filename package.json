{
  "name": "azurite",
  "version": "3.0.0-preview",
  "description": "A lightweight server clone of Azure Storage that simulates most of the commands supported by it with minimal dependencies.",
  "main": "index.js",
  "bin": {
    "azurite": "./blob/dist/server.js"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "dependencies": {
    "@azure/ms-rest-js": "^1.5.0",
    "express": "^4.16.4",
    "tslib": "^1.9.3",
    "uri-templates": "^0.2.0",
    "winston": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/uri-templates": "^0.1.29",
    "@types/express": "^4.16.0",
    "@types/xml2js": "^0.4.3",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.1.4"
  },
  "scripts": {
    "build": "tsc",
    "build:autorest": "autorest ./blob/swagger/README.md --typescript --use=E:/GitHub/XiaoningLiu/autorest.typescript.server",
    "build:autorestdebug": "autorest ./blob/swagger/README.md --typescript --typescript.debugger --use=E:/GitHub/XiaoningLiu/autorest.typescript.server",
    "clean": "rimraf dist typings *.log",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azure/azurite.git"
  },
  "keywords": [
    "Azurite",
    "Azure",
    "Storage",
    "Blob",
    "Queue",
    "Table",
    "Emulator",
    "Microsoft"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azure/azurite/issues"
  },
  "homepage": "https://github.com/azure/azurite#readme"
}
